    <script src="{{#if localAssets}}/static/assets/bootstrap.min.js{{else}}https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js{{/if}}" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
